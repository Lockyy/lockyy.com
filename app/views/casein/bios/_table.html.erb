<!-- Scaffolding generated by Casein v.4.0.0 -->

<table cellpadding="0" cellspacing="0" id="itemList" class="itemList">
	<tr>
		<th>ID</th>
		<th>Content</th>
		<th>Order</th>
		<th>&nbsp;</th>
	</tr>
	
	<% bios.each_with_index do |bio, index| %>
		<tr class="<%= 'even' if index.modulo(2) == 0 %> hover">
			<td><%= bio.id %></td>
			<td><%= casein_table_cell_link bio.content, casein_bio_path(bio) %></td>
			<td><%= casein_table_cell_link bio.order, casein_bio_path(bio) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("delete"), casein_bio_path(bio), :method => :delete, :data => { :confirm => "Are you sure you want to delete this bio?" }) %>
			</td>
		</tr>
	<% end %>
</table>